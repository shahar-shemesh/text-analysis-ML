    "##tokenizer המוציא רק מילים ללא מספרים או סימני פיסוק\n",
    "\n",
    "def my_tokenizer(txt):\n",
    "    new_text = txt.split(' ')\n",
    "    tokens = []\n",
    "    for token in new_text:\n",
    "        i = re.sub(r'([^\\w\\s])', '', token)\n",
    "        i = re.sub(r'\\d', '', i)\n",
    "        if i == '':\n",
    "            continue\n",
    "        else: tokens.append(i)\n",
    "    return tokens"
   ]
  },
